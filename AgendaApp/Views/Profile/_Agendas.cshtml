@model List<DashboardAgendaVM>

@if (Model.Count > 0)
{
    @foreach (var agenda in Model)
    {
        @await Html.PartialAsync("_Agenda", agenda)
    }
}
else
{
    <p class="lead text-center mt-5">
        <a asp-controller="Agenda" asp-action="Index">Get started by adding your first agenda now</a>
    </p>
}